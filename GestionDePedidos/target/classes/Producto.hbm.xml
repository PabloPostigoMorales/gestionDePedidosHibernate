<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.gestiondepedidos.model.producto.Producto" table="producto" schema="gestorpedidos">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="prod_Name">
            <column name="prod_Name" sql-type="varchar(20)"/>
        </property>
        <property name="price">
            <column name="price" sql-type="int(11)"/>
        </property>
        <property name="avail_Quantity">
            <column name="avail_quantity" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>